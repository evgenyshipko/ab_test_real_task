  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --print-label --parallel start:devServer start:devApi",
    "start:devServer": "webpack serve --config webpack/webpack.dev.js",
    "start:devApi": "nodemon",
    "production": "cross-env NODE_ENV=production npm-run-all --print-label --parallel start:server start:api",
    "start:server": "node server.js",
    "start:api": "ts-node --project api/tsconfig.json api/src/index.ts",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js"
  },
